import java.util.Arrays; 
 public class Main
 {
 public static void main(String[] args)
 {
    int[] arr = {89,5,29};
	System.out.println("Original Array: "+Arrays.toString(arr)); 
	int a = arr[0];
	arr[0] = arr[arr.length-1];
	arr[arr.length-1] = a;
	System.out.println("After swaping the first and last elements of the array are : "+Arrays.toString(arr));  
 }
}
