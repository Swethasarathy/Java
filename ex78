import java.util.Arrays; 
 public class Main 
 {
 public static void main(String[] args)
 {
    int[] arr = {5, 7};
	System.out.println("Original Array: "+Arrays.toString(arr)); 
	if(arr[0] == 4 || arr[0] == 7)
		System.out.println("True");
	else
    System.out.println(arr[1] == 4 || arr[1] == 7);
	 
 }
}
