import java.util.Scanner;
public class Main {
     public static void main(String[] args) {
		int a = 0;
		for(int x = 1; x <= 4; x++)
		{
			for(int y = 1; y <= 4; y++){
				for(int z = 1; z <= 4; z++)
				{
					if((z!=x)&&(z!=y) && (x!=y))
					{
				        a++;
						System.out.println(x + "" + y + "" + z);
					}
				}
			}
		}
		System.out.println("Total number of the three-digit-number is " + a);
	}
}
