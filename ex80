import java.util.Arrays; 
 public class Main
 {
 public static void main(String[] args)
 {
    int[] arr = {29, 89, 5};
	System.out.println("Original Array: "+Arrays.toString(arr)); 
	int result = arr[0];
	if(arr[2] >= result)
		result = arr[2];
	System.out.println("Larger value between first and last element of the array is : "+result); 	 
 }
}
